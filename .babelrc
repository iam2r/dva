{
    "presets": [
        "react",
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "plugins": [
        [
            "transform-runtime"
        ],
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
        "transform-react-jsx",
        [
            "transform-decorators-legacy"
        ]
    ]
}